[Unit]
Description=Suspend: rmmod ath11k_pci
Before=sleep.target

# [Service]
# Type=simple
# ExecStart=/usr/bin/rmmod ath11k_pci

[Service]
Type=oneshot
ExecStartPre=/usr/bin/modprobe -r ath11k_pci
# ExecStartPre=/usr/bin/modprobe -r ath11k
ExecStart=/usr/bin/sleep 1
# ExecStartPost=/usr/bin/modprobe ath11k
ExecStartPost=/usr/bin/modprobe ath11k_pci
# RemainAfterExit=yes

[Install]
WantedBy=sleep.target
