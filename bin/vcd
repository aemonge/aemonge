#!/bin/sh

if type nvr >/dev/null; then
    nvr -c "cd $(pwd)"
    # nvr -c "cd $(pwd) | lua require('swenv.api').auto_venv()"
else
    if [[ "$OSTYPE" == "darwin"* ]]; then
        printf ']51;["lua", "Tapi_Cd", "%s"]' "$(pwd)"
    else
        printf ']51;["lua", "Tapi_Cd", "%s"]' "$(echo -n $(pwd))"
    fi
fi
