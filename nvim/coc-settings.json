{
  "tree.key.selectNext": "<c-n>",
  "tree.key.selectPrevious": "<c-p>",
  "list.nextKeymap": "<c-n>",
  "list.previousKeymap": "<c-p>",
  "diagnostic.checkCurrentLine": true,
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "python.linting.pep8Enabled": true,
  "workspace.rootPatterns": [
    ".env",
    ".git",
    ".hg",
    ".bzr",
    ".svn",
    "Makefile",
    "package.json",
    "requirements.txt",
    "package.json",
    "Gemfile",
    "environment.yml",
    "README.rst",
    "README.md",
    "pyproject.toml",
    "tox.ini"
  ],
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.errorSign": "戀",
  "diagnostic.warningSign": "⚠",
  "diagnostic.hintSign": "♥",
  "diagnostic.infoSign": "כֿ",
  "hover.floatConfig": {
    "border": true,
    "rounded": true
  },
  "diagnostic.enableHighlightLineNumber": false,
  "diagnostic.floatConfig": {
    "border": true,
    "rounded": true
  },
  "suggest.virtualText": true,
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.removeDuplicateItems": true,
  "suggest.floatConfig": {
    "border": true,
    "rounded": true
  },
  "codeLens.enable": true,
  "colors.enable": true,
  "suggest.selection": "first",
  "suggest.autoTrigger": "always",
  "suggest.insertMode": "replace",

  "tabnine.priority": 80,
  "snippets.priority": 90,
  "coc.source.file.priority": 10,
  "codeLens.position": "right_align",

  "languageserver": {
    "sourcery": {
      "command": "sourcery",
      "args": ["lsp"],
      "filetypes": ["python"],
      "initializationOptions": {
        "extension_version": "coc.nvim",
        "editor_version": "neovim"
      },
      "settings": {
        "sourcery": {
          "metricsEnabled": true
        }
      }
    }
  },
  "cSpell.import": ["@cspell/dict-es-es/cspell-ext.json"],
  "cSpell.dictionaries": [
      "es_US",
      "softwareTerms",
      "companies",
      "python",
      "html",
      "css",
      "bash",
      "latex",
      "node",
      "filetypes"
  ],
  "Spell.enabledLanguageIds": [
    "es_es",
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "es",
    "git-commit",
    "gitcommit",
    "go",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "python",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "vim",
    "yaml",
    "yml"
  ]
}
